<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="eeepay.androidmorefunctiondemo">

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 调用摄像头的权限 -->
    <uses-permission android:name="android.permission.CAMERA" />
    <!-- 文件系统权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <!-- SD卡写权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <!-- Test -->
    <!-- MainActivity -->
    <application
        android:name=".TestApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".photo.WXPhotoAct" />
        <activity android:name=".photo.MainActivity" />
        <activity android:name=".photo.MainActivity2" />
        <activity android:name=".photo.AlbumActivity" />
        <activity android:name=".photo.ImageFile" />
        <activity android:name=".photo.ShowAllPhoto" />
        <activity android:name=".photo.GalleryActivity" />
        <activity android:name=".device.EncryptActivity" />
        <activity android:name=".des.EncryptAct" />
        <activity android:name=".CacheActivity" />
        <activity
            android:name=".html.CameraWebviewActivity"
            android:label="@string/title_activity_camera_webview" />
        <activity
            android:name=".phototwo.ShowPicActivity"
            android:label="@string/title_activity_show_pic"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".phototwo.ActivityCapture"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".html.CameraWeviewAct"
            android:label="@string/title_activity_camera_weview"
            android:theme="@style/AppTheme" />

        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="${UMENG_CHANNEL_VALUE}" />

        <activity android:name=".perfectlayout.PerfectActivity" />
        <activity android:name=".intentapp.MoreIntentActivity"></activity>
    </application>

</manifest>